  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=\"production\" ts-node -r tsconfig-paths/register ./src/main.ts",
    "dev": "cross-env NODE_ENV=\"development\" ts-node-dev -r tsconfig-paths/register ./src/main.ts",
    "compile": "tsc -p .",
    "compile:test": "tsc -p . --noEmit",
    "package": "run-s clean compile packing",
    "packing": "ncc build ./dist/main.js -o output/ -m -s",
    "clean": "rimraf dist output",
    "lint": "run-p -c lint:prettier lint:eslint lint:tsc",
    "lint:prettier": "prettier --check src",
    "lint:eslint": "eslint . --ext ts,tsx",
    "lint:tsc": "tsc",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "eslint . --ext ts,tsx --fix",
    "fix:prettier": "prettier --write src"
  },
  "devDependencies": {
    "@discordjs/builders": "1.5.0",
    "@types/config": "3.3.0",
    "@types/node": "18.15.3",
    "@types/node-cron": "3.0.7",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "@vercel/ncc": "0.36.1",
    "config": "3.3.9",
    "cross-env": "7.0.3",
    "discord.js": "14.8.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-promise": "6.1.1",
    "mysql2": "2.3.3",
    "node-cron": "3.0.2",
    "prettier": "2.8.5",
    "reflect-metadata": "0.1.13",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typeorm": "0.3.12",
    "typeorm-naming-strategies": "4.1.0",
    "typescript": "5.0.2",
    "yarn-run-all": "3.1.1"
  }
}
