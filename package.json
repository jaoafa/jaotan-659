  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=\"production\" ts-node -r tsconfig-paths/register ./src/main.ts",
    "dev": "cross-env NODE_ENV=\"development\" ts-node-dev -r tsconfig-paths/register ./src/main.ts",
    "compile": "tsc -p .",
    "compile:test": "tsc -p . --noEmit",
    "package": "run-s clean compile packing",
    "packing": "ncc build ./dist/main.js -o output/",
    "clean": "rimraf dist output",
    "lint": "run-p -c lint:prettier lint:eslint lint:tsc",
    "lint:prettier": "prettier --check src",
    "lint:eslint": "eslint . --ext ts,tsx",
    "lint:tsc": "tsc",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "eslint . --ext ts,tsx --fix",
    "fix:prettier": "prettier --write src"
  },
  "devDependencies": {
    "@discordjs/builders": "1.6.5",
    "@types/config": "3.3.1",
    "@types/node": "18.17.15",
    "@types/node-cron": "3.0.8",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "@vercel/ncc": "0.38.0",
    "config": "3.3.9",
    "cross-env": "7.0.3",
    "discord.js": "14.11.0",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-n": "16.1.0",
    "eslint-plugin-promise": "6.1.1",
    "mysql2": "3.6.1",
    "node-cron": "3.0.2",
    "prettier": "2.8.8",
    "reflect-metadata": "0.1.13",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typeorm": "0.3.17",
    "typeorm-naming-strategies": "4.1.0",
    "typescript": "5.2.2",
    "yarn-run-all": "3.1.1"
  }
